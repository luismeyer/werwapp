{
	"name": "werwapp",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "yarn gen:licenses && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"gen:licenses": "license-report --output=json > ./static/licenses.json",
		"lint": "eslint . --fix",
		"lint:check": "eslint .",
		"prettier": "prettier --plugin-search-dir . --write .",
		"prettier:check": "prettier --plugin-search-dir . --check .",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest"
	},
	"dependencies": {
		"daisyui": "^2.51.5",
		"string-similarity": "^4.0.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-cloudflare": "^2.2.0",
		"@sveltejs/kit": "^1.21.0",
		"@types/dom-screen-wake-lock": "^1.0.1",
		"@types/string-similarity": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"license-report": "^6.3.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.58.0",
		"svelte-check": "^3.4.4",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.2.1",
		"vitest": "^0.32.2"
	}
}
